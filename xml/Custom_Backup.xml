<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1104">
	<!-- yesnodialog -->
	<defaultcontrol always="true">10</defaultcontrol>
	
	<controls>

		<!-- Dialog fanart -->
		<include>DialogFanart</include>
		
		<control type="group">

			<!-- Animation -->
			<include>DialogZoomAnimation</include>

			<!-- Heading -->
			<include content="Time">
				<param name="heading">$LOCALIZE[31168]</param>
			</include>

			<!-- Heading -->
			<control type="label" id="1">
				<left>-15000</left>
				<top>40</top>
				<width>600</width>
				<height>48</height>
				<align>center</align>
				<font>Font48</font>
				<textcolor>$VAR[TextColor1]</textcolor>
			</control>

			<!-- Details -->
			<control type="group">
				<control type="textbox">
					<include>Custom_Backup_coords</include>
					<align>center</align>
					<aligny>center</aligny>
					<font>Font36</font>
					<autoscroll delay="10000" time="1700" repeat="12000">True</autoscroll>
					<textcolor>$VAR[TextColor1]</textcolor>
					<label>$LOCALIZE[31150]</label>
				</control>
			</control>

			<!-- Button grouplist background -->
			<include content="dialogButtonBackground">
				<param name="id">9001</param>
			</include>

			<!-- Buttons -->
			<control type="grouplist" id="9001">
				<include>DialogButtons</include>
				<orientation>horizontal</orientation>
				<align>center</align>
				<itemgap>30</itemgap>
				<control type="button" id="10">
					<width>auto</width>
					<font>Font36</font>
					<align>center</align>
					<onclick>Dialog.Close(1104)</onclick>
					<label>$LOCALIZE[31151]</label>
				</control>
				<control type="button" id="11">
					<width>auto</width>
					<font>Font36</font>
					<align>center</align>
					<onclick>Dialog.Close(1104)</onclick>
					<onclick>ActivateWindow(addonbrowser)</onclick>
					<label>$LOCALIZE[31152]</label>
				</control>
			</control>
			
		</control>

		<include>MaskingBars</include>

	</controls>

</window>
